<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.com.mapper.ItemOrderMapper">
<insert id="insertItemOrder">
INSERT INTO ITEMORDER VALUES(ono_seq.nextval, #{userId}, #{ino}, #{quantity})
</insert>

<delete id="deleteItemOrder">
DELETE FROM ITEMORDER WHERE ono = #{ono}

</delete>
</mapper>